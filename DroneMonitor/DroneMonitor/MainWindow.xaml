<Window x:Class="DroneMonitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DroneMonitor"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="lblTitel" Content="Drone Monitor" HorizontalAlignment="Center" Margin="206,21,222,0" VerticalAlignment="Top"/>
        <Label x:Name="lblTemp" Content="Temperature :" HorizontalAlignment="Left" Margin="27,74,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblWindSpeed" Content="Wind Speed :" HorizontalAlignment="Left" Margin="27,105,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblWindOrientation" Content="Wind Oriantation :" HorizontalAlignment="Left" Margin="27,136,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.526,-0.105"/>
        <Label x:Name="lblParticle" Content="Particle Size :" HorizontalAlignment="Left" Margin="27,167,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblTime" Content="Timestamp :
               " HorizontalAlignment="Left" Margin="27,198,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.738,0.598"/>

    </Grid>
</Window>
